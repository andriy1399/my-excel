{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "arrow-parens":"off",
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "require-jsdoc": "off",
        "object-curly-spacing": ["error", "always"]
    },
    "extends": ["eslint:recommended", "google"]
}